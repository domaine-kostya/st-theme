{
  "name": "southerntide",
  "version": "1.0.0",
  "main": "webpack.config",
  "author": "Domaine",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lighthouse": "lhci autorun",
    "shopify:logout": "shopify auth logout",
    "start": "run-p -sr dev webpack:watch",
    "start:staging": "run-p -sr staging webpack:watch",
    "build": "run-s webpack:build",
    "dev": "source .env && shopify theme dev -s southerntidellc --path=shopify",
    "staging": "source .env && shopify theme dev -s southerntidellc-dev --path=shopify",
    "lint": "run-s -c lint:*",
    "lint:js": "eslint \"./src/scripts/**/*.js\" --config ./.eslintrc.js",
    "lint:css": "stylelint \"./src/styles/**/*.{css,scss}\" --config ./.stylelintrc.js",
    "lint:shopify": "source .env && shopify theme check -s southerntidellc --path=shopify",
    "webpack:watch": "cross-env NODE_ENV=development webpack --watch --progress",
    "webpack:build:dev": "cross-env NODE_ENV=development webpack --progress",
    "webpack:build": "cross-env NODE_ENV=production webpack --progress"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@shopify/cli": "3.48.1",
    "@shopify/theme": "3.48.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "commander": "^11.0.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.33.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "glob": "^10.3.10",
    "mini-css-extract-plugin": "^2.7.6",
    "mini-svg-data-uri": "^1.4.4",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "sass-loader": "^13.3.2",
    "sass-resources-loader": "^2.2.5",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-webpack-plugin": "^4.1.1",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0",
    "webpack-remove-empty-scripts": "^1.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "@lhci/cli": "^0.12.0",
    "@lottiefiles/lottie-player": "^2.0.2",
    "@shopify/theme-a11y": "^4.1.1",
    "@shopify/theme-addresses": "1.0.0",
    "@shopify/theme-currency": "^4.1.1",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "babelify": "^7.3.0",
    "loop.js": "^0.1.0",
    "nanoajax": "^0.4.3",
    "choices.js": "^10.2.0",
    "focus-within-polyfill": "^5.2.1",
    "js-cookie": "^3.0.5",
    "sass": "^1.77.0",
    "swiper": "5.4.5"
  }
}
